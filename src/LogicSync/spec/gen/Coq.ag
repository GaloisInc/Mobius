
CoqSpecific :> Require, OpenScope, Coercion, Hint, Tactic;

Require = String! lib, ReqType! type;
Coercion = String! name, String! typeFrom, String! typeTo;
OpenScope = String! name;
Tactic = String! name, String! content;
Hint = HintType! type, List<String>! list, String! lib;


Axiom = AxiomType! type, String! name, Formula! formula;
Lemma = String! name, Formula! formula, String proof;
Definition = String! name, Formula type, Formula def, String proof;
Inductive = String! name, Formula! type, ConstrList! list;

Documentation :> Doc, Comment;
Doc = String! content;
Comment = String! content;
Variable = Variable next, String name, Formula type;
VariableList = Variable! first, Variable! tail;

Formula :> Application, Term, Forall, Fun, Exists, BinaryTerm;
Formula = Formula next;
Application = Formula! first;
BinaryTerm = Formula! op, Formula! left, Formula! right;
Term = String name;
Forall = VariableList! list, Formula! formula;
Fun = VariableList! list, Formula! formula;
Exists = Variable! list, Formula! formula;

Constructor = Constructor next, String! name, Formula! type;
ConstrList = Constructor first, Constructor last;
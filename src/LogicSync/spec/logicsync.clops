NAME::
  LogicSync

ARGS::
  Help: {"-h", "-help", "--help"}
  		: "Show this help message."
  TypeCheck: {"-tc"} : "Properly prints the typechecking result."  
  Files: {}:{file-list}:[between="", mustExist="true", canBeDir="false", allowMultiple="true",argumentname="<files>"]
  		: "Provide input file(s). Determine their type by their extension."
  Generate: {"-g"}:{file-list}:[canBeDir="false"]
      : "Generates files. Files will erase the previous version."
  Merge: {"-m"}:{file-list}:[canBeDir="false"]
      : "Merge with existing file. If there is no previous version, creates a new file."

FORMAT::
  Help  | ((Output)* TypeCheck? Files (Files | Output)*);  
 
WHERE::
  Main:
    Help | Files;
  Output:
    Generate | Merge;

	
<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="MobiusPVE"
         name="Mobius PVE"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="mobiuspve.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="mobiuspve.Perspective"
            id="MobiusPVE.perspective">
      </perspective>
   </extension>
   <extension
         id="MobiusPVE"
         name="Mobius PVE"
         point="org.eclipse.core.runtime.products">
      <product
            application="MobiusPVE.application"
            name="Mobius PVE">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
      </product>
   </extension>

</plugin>

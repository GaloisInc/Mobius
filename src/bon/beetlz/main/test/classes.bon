  class BLACK_CLASS
  feature
    nice: BOOLEAN
    counter: INTEGER
    maxcount: INTEGER
      ensure
        Result = 23;
      end
    name: STRING
      ensure 
        Result = "name";
      end
    currentColour: STRING
    colours: SEQUENCE[STRING]
    numbers: SEQUENCE[VALUE]
    
    one: STRING
      -> eins: STRING
      -> zwei: STRING
      require 
        zwei.length > 5;
        zwei.length < 15;
        zwei.length >= 5;
        zwei.length <= 15;
        zwei /= Void;
      ensure
        delta nice;
        delta numbers;
        numbers.item(0) /= 0;
        numbers.item(1) = 0;
      end
      
    two: INTEGER
      -> eins: VALUE
      -> zwei: VALUE
      -> drei: VALUE
      require
        eins + zwei < 100;
         eins-zwei > 0;
         eins * 5 = zwei / 3;
      ensure
        Result \\ 2 = 0;
         - (Result) <= 0;
         eins ^ zwei = Result;
         Result = old eins + old zwei;
        delta {colours, counter, currentColour};
      end
      
    three: YELLOW_CLASS
      -> eins: BOOLEAN
      -> zwei: BOOLEAN 
      -> drei: BOOLEAN
      require
        not eins;
        eins and zwei -> drei;
      ensure 
        (eins and zwei) or drei <-> drei and nice;
        not ((nice and zwei) <-> (counter < 0));
        (Result).find("hallo"); 
      end
        
    end
    
    
    class YELLOW_CLASS
    feature
        find: BOOLEAN
        -> s: STRING        
    end

indexing
  about: "An alarm.";

static_diagram
component
  deferred class ALARM -- An alarm that is either on or off.
  feature
    deferred on  -- Turn this alarm on.
      ensure
        is_on();
      end
    deferred off  -- Turn this alarm off.
      ensure
        not is_on();
      end
    deferred is_on: BOOLEAN  -- Is this alarm on?
  end -- class ALARM
end

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="Annotationpropagation"
   name="Annotation propagation Plug-in"
   version="1.0.7"
   provider-name=""
   class="Annotationpropagation.AnnotationpropagationPlugin">

   <runtime>
      <library name="Annotationpropagation.jar"/>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="jack.plugin"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.eclipse.compare"/>
      <import plugin="JackRequiredPlugins"/>
   </requires>


   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="*.prop"
            id="propagate.annotation">
         <action
               class="propagation.PropagateAction"
               enablesFor="1"
               icon="icons/filter_ps.gif"
               id="propagation.propagateAction"
               label="Propagate annotations"
               menubarPath="additions">
         </action>
      </objectContribution>
   </extension>

</plugin>
